{
  "expo": {
    "name": "match-sounds",
    "slug": "match-sounds",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pads.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/pads.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000",
      "resizeWidth": 200, 
      "resizeHeight": 200

    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
